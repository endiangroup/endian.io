// {{.Site.Params.headerComment | safeHTML }}
= doctype html
html lang={{.Site.LanguageCode}}
  {{ partial "head" . }}

  body
    .container
      {{ partial "navigation" "articles" }}
      header.compact
        section
          = yield header
      article
        section
          = yield main
      footer
        {{ partial "footer" . }}

  {{ partial "javascript" . }}
